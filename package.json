{
  "name": "phoenix",
  "version": "2.6.3",
  "private": true,
  "devDependencies": {
    "grunt": "1.0.3",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-uglify": "4.0.0",
    "jscs": "3.0.7",
    "jshint": "2.9.7"
  },
  "scripts": {
    "build": "npm run clean && npm test && grunt",
    "clean": "rm -frv .grunt/ Phoenix/$npm_package_name.js Phoenix/$npm_package_name.min.js",
    "lint": "jshint --config jshint.json Gruntfile.js library/ && jscs --config jscs.json Gruntfile.js library/",
    "test": "npm run lint"
  },
  "engines": {
    "node": ">=11.5.0",
    "npm": ">=6.5.0"
  }
}
